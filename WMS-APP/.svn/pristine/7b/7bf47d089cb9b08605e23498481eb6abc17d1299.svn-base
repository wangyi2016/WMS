package com.mjzk.agv.pojo;

import java.util.List;

/**
 * 分页数据集
 * 
 * @author XieChangJiang
 *
 * @param <T>
 */
public class Data<T> {

	public int total;
	public List<T> rows;

	public Data() {
		// TODO Auto-generated constructor stub
	}

	public Data(int total, List<T> rows) {
		this.total = total;
		this.rows = rows;
	}

	public int getTotal() {
		return total;
	}

	public void setTotal(int total) {
		this.total = total;
	}

	public List<T> getRows() {
		return rows;
	}

	public void setRows(List<T> rows) {
		this.rows = rows;
	}
}
