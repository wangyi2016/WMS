<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>添加AGV</title>
<style type="text/css">

</style>
</head>
<body>

<div style="margin-top:10px;" id="bo">
		     <table id="agvlist">
		</table>
	</div>

<form id="manage_add" style="margin:0 ; padding:5px 0 0 25px;color:#333">
  <div style="font-size:12px;">请正确填写下列信息！</div>
  <div style="text-align:center;margin-top:15px">
  <table style=" margin:0 auto; margin-bottom:15px;">
        <tr style="padding:7px;">
            <td style="text-align:right;">编号：</td><td><input type="text" name="agv_code" class="textbox" style="width:160px;" id="code"> </td><td id="ercode"></td>
        </tr>
        <tr style="padding:7px;">
            <td style="text-align:right;">描述：</td><td><input type="text" name="agv_desc" class="textbox" style="width:160px;" id="desc"> </td><td id="erdesc"></td>
        </tr>
        <tr>
            <td style="text-align:right;">IP地址：</td><td><input type="text" name="ip_Adr" class="textbox" style="width:160px;" id="ip"> </td><td id="erip"></td>
        </tr>
        <tr>
            <td style="text-align:right;">生产线：</td><td><input type="text" name="product_num" class="textbox" style="width:160px;" id="prod"> </td><td id="erprod"></td>
        </tr>
        <tr>
            <td style="text-align:right;">初始速度：</td><td><input type="text" name="initSpd" class="textbox" style="width:160px;" id="spd"></td><td id="erspd"></td>
        </tr>
         <tr>
            <td style="text-align:right;">是否立即启用：</td>
            <td><select class="easyui-combobox" name="state" style="width:164px;" id="go">
              <option value="1" selected>是</option>
              <option value="2">否</option>
             </select></td>
            <td id="erspd"></td>
        </tr>
  </table>

  </div>  
</form>

	<script type="text/javascript" src="js/agv/addagv.js"></script>
</body>
</html>